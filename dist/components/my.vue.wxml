<template name="f9e8d82a">
  <view class="_div data-v-99edd19a my-content">
    <view class="_div data-v-99edd19a my-background"></view>
    <view class="_div data-v-99edd19a user-detail">
      <view class="_div data-v-99edd19a user-head-image"></view>
      <view class="_div data-v-99edd19a user-name"></view>
    </view>
    <view class="_div data-v-99edd19a user-song">
      <view class="_div data-v-99edd19a my-item">
        <view class="_div data-v-99edd19a item-icon">
          <view class="_i data-v-99edd19a flaticon-musical-note"></view>
        </view>
        <view class="_div data-v-99edd19a item-detail">本地音乐</view>
      </view>
      <view class="_div data-v-99edd19a my-item">
        <view class="_div data-v-99edd19a item-icon">
          <view class="_i data-v-99edd19a flaticon-heart"></view>
        </view>
        <view class="_div data-v-99edd19a item-detail">我喜欢的</view>
      </view>
      <view class="_div data-v-99edd19a my-item">
        <view class="_div data-v-99edd19a item-icon">
          <view class="_i data-v-99edd19a flaticon-passage-of-time"></view>
        </view>
        <view class="_div data-v-99edd19a item-detail">最近播放</view>
      </view>
      <view class="_div data-v-99edd19a songlist">
        <view class="_div data-v-99edd19a">
          <view class="_i data-v-99edd19a flaticon-right-arrow"></view>
        </view>
        <view class="_div data-v-99edd19a songname">创建的歌单({{songlist.length + 1}})</view>
        <view class="_div data-v-99edd19a">
          <view class="_i data-v-99edd19a flaticon-settings"></view>
        </view>
      </view>
      <view class="_div data-v-99edd19a user-song-item">
        <view class="_div data-v-99edd19a song-item" wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{songlist}}" wx:for-index="index" wx:for-item="item">
          <view class="_div data-v-99edd19a song-left">
            <image src="{{item.coverImgUrl}}" alt class="_img data-v-99edd19a"></image>
          </view>
          <view class="_div data-v-99edd19a song-right">
            <view class="_div data-v-99edd19a song-detail">
              <view class="_p data-v-99edd19a">{{item.name}}</view>
              <view class="_p data-v-99edd19a">{{item.trackCount}}</view>
            </view>
            <view class="_div data-v-99edd19a">
              <view class="_i data-v-99edd19a flaticon-more"></view>
            </view>
          </view>
        </view>
      </view>
      <view class="_div data-v-99edd19a songlist">
        <view class="_div data-v-99edd19a">
          <view class="_i data-v-99edd19a flaticon-right-arrow"></view>
        </view>
        <view class="_div data-v-99edd19a songname">收藏的歌单({{Collectionsong.length + 1}})</view>
        <view class="_div data-v-99edd19a">
          <view class="_i data-v-99edd19a flaticon-settings"></view>
        </view>
      </view>
      <view class="_div data-v-99edd19a user-collectionsong">
        <view class="_div data-v-99edd19a song-item" wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'1-'+index}}" data-comkey="{{$k}}" wx:for="{{Collectionsong}}" wx:for-index="index" wx:for-item="item">
          <view class="_div data-v-99edd19a song-left">
            <image src="{{item.coverImgUrl}}" alt class="_img data-v-99edd19a"></image>
          </view>
          <view class="_div data-v-99edd19a song-right">
            <view class="_div data-v-99edd19a song-detail">
              <view class="_p data-v-99edd19a">{{item.name}}</view>
              <view class="_p data-v-99edd19a">{{item.trackCount}}</view>
            </view>
            <view class="_div data-v-99edd19a">
              <view class="_i data-v-99edd19a flaticon-more"></view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>